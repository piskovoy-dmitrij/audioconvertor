= render 'friends_navigation'

%div.container-fluid
  %div.row
    = form_tag search_friends_path, method: 'get' do
      %div.input-group
        = text_field_tag :q, params[:q], class: 'form-control', placeholder: 'Search...'

        %span.input-group-btn
          = submit_tag :Search, class:"btn btn-default"

    = render 'users/list'